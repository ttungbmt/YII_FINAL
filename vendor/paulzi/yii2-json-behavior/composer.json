{
  "name": "paulzi/yii2-json-behavior",
  "description": "Yii2 json attribute behavior",
  "keywords": ["yii2", "json", "behavior", "validator"],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/paulzi/yii2-json-behavior/issues?state=open",
    "source": "https://github.com/paulzi/yii2-json-behavior"
  },
  "authors": [
    {
      "name": "PaulZi",
      "email": "pavel.zimakoff@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "paulzi\\jsonBehavior\\": "./src"
    }
  }
}
